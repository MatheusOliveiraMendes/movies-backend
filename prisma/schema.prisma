

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Movie {
  id          Int      @id
  key         String
  name        String
  description String
  genres      String[] 
  rate        Float
  length      String
  img         String
}
